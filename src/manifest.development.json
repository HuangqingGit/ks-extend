{
    "manifest_version": 2,
    "name": "Audit data",
    "description": "用于快速统计审核员实时审核数据",
    "version": "1.0.0",
    "options_page": "options.html",
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "img/128.png",
            "32": "img/128.png",
            "48": "img/128.png",
            "128": "img/128.png"
        }
    },
    "icons": {
        "16": "img/128.png",
        "32": "img/128.png",
        "48": "img/128.png",
        "128": "img/128.png"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}